<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd'T'HH:mm:ss.SSS} %-5level %logger{36} - %replace(%msg){'eyJ[0-9A-Za-z_-]+.[0-9A-Za-z_-]+.[0-9A-Za-z_-]+','**********'}%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
